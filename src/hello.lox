print "Hello World";

fun grade_score(score) {
    if (score > 100);
    else if (score >= 80) return "A";
    else if (score >= 70) return "B";
    else if (score >= 60) return "C";
    else if (score >= 50) return "D";
    else if (score >= 40) return "E";
    else if (score >= 0) return "F";
    return "Invalid Score: " + string(score);
}

print grade_score(45);
print grade_score(105);
print grade_score(-5);
print grade_score(90);
