class Zoo {
  init() {
    this.fox = 1;
    this.cat = 1;
    this.baboon = 1;
    this.donkey = 1;
    this.aardvark = 1;
    this.elephant = 1;
  }
  tuna() { return this.cat; }
  mouse() { return this.fox; }
  hay() { return this.donkey; }
  ant() { return this.aardvark; }
  banana() { return this.baboon; }
  grass() { return this.elephant; }
}

var zoo = Zoo();

var sum = 0;

var start = clock();

while (sum < 100000000)
  sum = sum
      + zoo.ant()
      + zoo.tuna()
      + zoo.mouse()
      + zoo.hay()
      + zoo.ant()
      + zoo.banana()
      + zoo.grass();

print clock() - start;
print sum;
