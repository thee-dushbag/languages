class ZooAn {
  init() {
    this.fox = 1;
    this.cat = 1;
    this.baboon = 1;
    this.donkey = 1;
    this.aardvark = 1;
    this.elephant = 1;
  }
  tuna() { return this.cat; }
  mouse() { return this.fox; }
  hay() { return this.donkey; }
  ant() { return this.aardvark; }
  banana() { return this.baboon; }
  grass() { return this.elephant; }
}

var zoo_an = ZooAn();

var tsum = 0;

var start = clock();

while (tsum < 100000000)
  tsum = tsum
      + zoo_an.ant()
      + zoo_an.tuna()
      + zoo_an.mouse()
      + zoo_an.hay()
      + zoo_an.ant()
      + zoo_an.banana()
      + zoo_an.grass();

print clock() - start;
print tsum;
